.i_wrap {
  width: 100%;
  height: 100vh;
}

.i_btnsWrap {
  width: 100%;
  margin-top: 200rpx;
}

.i_button {
  width: 500rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-weight: bold;
  border-radius: 80rpx;
  border: 1rpx solid #fff;
  text-align: center;
  color: #fff;
  margin: 0 auto;
  margin-bottom: 40rpx;
  position: relative;
}

.i_button button {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
}